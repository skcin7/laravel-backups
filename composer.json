{
    "name": "skcin7/laravel-backups",
    "description": "Package to manage database backups in a Laravel project. Easily integrates with Dropbox and AWS S3.",
    "keywords": ["LaravelBackups"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Morgan",
            "email": "nick@nicholas-morgan.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "spatie/flysystem-dropbox": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "skcin7\\LaravelDbBackup": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "skcin7\\LaravelDbBackup\\DBBackupServiceProvider"
            ]
        }
    }
}
